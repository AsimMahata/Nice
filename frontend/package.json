{
    "name": "frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "main": "electron/dist/main.cjs",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "electron:build": "esbuild electron/main.ts --bundle --platform=node --outfile=electron/dist/main.cjs --external:electron --external:node-pty && esbuild electron/preload.ts --bundle --platform=node --outfile=electron/dist/preload.cjs --external:electron --external:node-pty",
        "electron:dev": "npm run electron:build && NODE_ENV=development electron .",
        "electron:prod": "npm run build && npm run electron:build && electron .",
        "lint": "eslint .",
        "preview": "vite preview"
    },
    "dependencies": {
        "@monaco-editor/react": "^4.7.0",
        "@tailwindcss/vite": "^4.1.18",
        "@xterm/addon-fit": "^0.11.0",
        "@xterm/xterm": "^6.0.0",
        "axios": "^1.13.2",
        "lucide-react": "^0.563.0",
        "monaco-editor": "^0.55.1",
        "node-notifier": "^10.0.1",
        "node-pty": "^1.1.0",
        "os": "^0.1.2",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-hook-form": "^7.71.1",
        "react-resizable-panels": "^4.6.2",
        "react-router": "^7.12.0",
        "react-router-dom": "^7.12.0",
        "tailwindcss": "^4.1.18",
        "vscode-jsonrpc": "^8.2.0",
        "vscode-ws-jsonrpc": "^3.3.2"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@types/node": "^24.10.11",
        "@types/react": "^19.2.5",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.1",
        "electron": "^40.2.1",
        "esbuild": "^0.27.3",
        "eslint": "^9.39.1",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.24",
        "globals": "^16.5.0",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.46.4",
        "vite": "^7.2.4"
    }
}
