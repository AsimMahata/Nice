{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "fs:allow-read-dir",
      "allow": [
        {
          "path": "$HOME/dev/testing"
        },
        {
          "path": "$HOME/dev/testing/**"
        }
      ]
    },
    {
      "identifier": "fs:allow-exists",
      "allow": [
        {
          "path": "$HOME/dev/testing"
        },
        {
          "path": "$HOME/dev/testing/**"
        }
      ]
    },
    {
      "identifier": "fs:allow-mkdir",
      "allow": [
        {
          "path": "$HOME/dev/testing"
        },
        {
          "path": "$HOME/dev/testing/**"
        }
      ]
    },
    {
      "identifier": "fs:allow-write-text-file",
      "allow": [
        {
          "path": "$HOME/dev/testing"
        },
        {
          "path": "$HOME/dev/testing/**"
        }
      ]
    },
    {
      "identifier": "fs:read-files",
      "allow": [
        {
          "path": "$HOME/dev/testing"
        },
        {
          "path": "$HOME/dev/testing/**"
        }
      ]
    }
  ]
}
